/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@mm/environment": ["src/environments/environment.ts"],
      "@mm/texts": ["src/assets/texts/uk.json"],
      // Auth
      "@mm/auth/components": ["src/app/shared/auth/components/index.ts"],
      "@mm/auth/models": ["src/app/shared/auth/models/index.ts"],
      "@mm/auth/services": ["src/app/shared/auth/services/index.ts"],
      "@mm/auth/actions": ["src/app/shared/auth/store/actions/index.ts"],
      "@mm/auth/effects": ["src/app/shared/auth/store/effects/index.ts"],
      "@mm/auth/reducers": ["src/app/shared/auth/store/reducers/index.ts"],
      "@mm/auth/selectors": ["src/app/shared/auth/store/selectors/index.ts"],
      "@mm/auth/constants": ["src/app/shared/auth/store/constants/index.ts"],
      // Details
      "@mm/details": ["src/app/film-details/index.ts"],
      // Favourites
      "@mm/favourites/store": ["src/app/favourites/store/index.ts"],
      "@mm/favourites/actions": ["src/app/favourites/store/actions/index.ts"],
      "@mm/favourites/effects": ["src/app/favourites/store/effects/index.ts"],
      "@mm/favourites/selectors": ["src/app/favourites/store/selectors/index.ts"],
      // Shared Common
      "@mm/shared/decorators": ["src/app/shared/common/cdk/decorators/index.ts"],
      "@mm/shared/directives": ["src/app/shared/common/cdk/directives/index.ts"],
      "@mm/shared/pipes": ["src/app/shared/common/cdk/pipes/index.ts"],
      "@mm/shared/utils": ["src/app/shared/common/cdk/utils/index.ts"],
      "@mm/shared/services": ["src/app/shared/common/cdk/services/index.ts"],
      "@mm/shared/validators": ["src/app/shared/common/cdk/validators/index.ts"],
      "@mm/shared/common/models": ["src/app/shared/common/models/index.ts"],
      "@mm/shared/ui/breadcrumbs": ["src/app/shared/common/ui/breadcrumbs/index.ts"],
      "@mm/shared/ui/button": ["src/app/shared/common/ui/button/index.ts"],
      "@mm/shared/ui/icon": ["src/app/shared/common/ui/icon/index.ts"],
      "@mm/shared/ui/not-found": ["src/app/shared/common/ui/not-found/index.ts"],
      "@mm/shared/ui/pagination": ["src/app/shared/common/ui/pagination/index.ts"],
      "@mm/shared/ui/search-input": ["src/app/shared/common/ui/search-input/index.ts"],
      "@mm/shared/ui/spinner": ["src/app/shared/common/ui/spinner/index.ts"],
      "@mm/shared/ui/tabset": ["src/app/shared/common/ui/tabset/index.ts"],
      "@mm/shared/ui/title": ["src/app/shared/common/ui/title/index.ts"],
      "@mm/shared/ui/tooltip": ["src/app/shared/common/ui/tooltip/index.ts"],
      // Shared Core
      "@mm/shared/core": ["src/app/shared/core/index.ts"],
      // Shared Favourites
      "@mm/shared/favourites/api": ["src/app/shared/favourites/api/index.ts"],
      "@mm/shared/favourites/actions": ["src/app/shared/favourites/store/actions/index.ts"],
      "@mm/shared/favourites/effects": ["src/app/shared/favourites/store/effects/index.ts"],
      "@mm/shared/favourites/models": ["src/app/shared/favourites/store/models/index.ts"],
      "@mm/shared/favourites/reducers": ["src/app/shared/favourites/store/reducers/index.ts"],
      "@mm/shared/favourites/selectors": ["src/app/shared/favourites/store/selectors/index.ts"],
      // Shared films
      "@mm/shared/films/api": ["src/app/shared/films/api/index.ts"],
      "@mm/shared/films/models": ["src/app/shared/films/models"],
      "@mm/shared/films/section": ["src/app/shared/films/section/index.ts"],
      "@mm/shared/films/store": ["src/app/shared/films/store/index.ts"],
      "@mm/shared/films/actions": ["src/app/shared/films/store/actions/index.ts"],
      "@mm/shared/films/effects": ["src/app/shared/films/store/effects/index.ts"],
      "@mm/shared/films/reducers": ["src/app/shared/films/store/reducers/index.ts"],
      "@mm/shared/films/selectors": ["src/app/shared/films/store/selectors/index.ts"],
      "@mm/shared/films/state": ["src/app/shared/films/store/state/index.ts"],
      "@mm/shared/films/ui": ["src/app/shared/films/ui/index.ts"],
      "@mm/shared/films/utils": ["src/app/shared/films/utils/index.ts"],
      "@mm/shared/forms": ["src/app/shared/forms/index.ts"],
      "@mm/shared/models": ["src/app/shared/models/index.ts"],
      "@mm/shared/store": ["src/app/shared/store/index.ts"],
      "@mm/shared/actions": ["src/app/shared/store/actions/index.ts"],
      "@mm/shared/effects": ["src/app/shared/store/effects/index.ts"],
      "@mm/shared/helpers": ["src/app/shared/store/helpers/index.ts"],
      "@mm/shared/store/models": ["src/app/shared/store/models/index.ts"],
      "@mm/shared/reducers": ["src/app/shared/store/reducers/index.ts"],
      "@mm/shared/selectors": ["src/app/shared/store/selectors/index.ts"],
      "@mm/shared/state": ["src/app/shared/store/state/index.ts"],
      // App store
      "@mm/store": ["src/app/store/uk.json"]
    },
    "outDir": "./dist/out-tsc",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "resolveJsonModule": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": ["ES2022", "dom"]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
